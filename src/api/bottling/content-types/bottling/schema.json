{
  "kind": "collectionType",
  "collectionName": "bottlings",
  "info": {
    "singularName": "bottling",
    "pluralName": "bottlings",
    "displayName": "Bottling"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bottlingId": {
      "type": "customField",
      "required": true,
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::item.item"
    },
    "bottlingDateTime": {
      "type": "datetime"
    },
    "startDateTime": {
      "type": "datetime"
    },
    "endDateTime": {
      "type": "datetime"
    },
    "bottlingCount": {
      "type": "integer"
    },
    "defectiveCount": {
      "type": "integer"
    },
    "comment": {
      "type": "text"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
