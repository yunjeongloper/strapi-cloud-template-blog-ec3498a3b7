{
  "kind": "collectionType",
  "collectionName": "spirits",
  "info": {
    "singularName": "spirit",
    "pluralName": "spirits",
    "displayName": "Spirit",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "spiritId": {
      "type": "customField",
      "required": true,
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "serialNumber": {
      "type": "string"
    },
    "spiritStatus": {
      "type": "enumeration",
      "enum": [
        "Distilled",
        "Blended",
        "Aged",
        "Expired",
        "Disposed"
      ]
    },
    "startAt": {
      "type": "datetime"
    },
    "endAt": {
      "type": "datetime"
    },
    "batchNumber": {
      "type": "string"
    },
    "abv": {
      "type": "decimal"
    },
    "initialVolume": {
      "type": "integer"
    },
    "currentVolume": {
      "type": "integer"
    },
    "currentLocation": {
      "type": "string"
    },
    "proofings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::proofing.proofing",
      "mappedBy": "spirit_id"
    },
    "bottlings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bottling.bottling",
      "mappedBy": "spirit"
    },
    "comment": {
      "type": "text"
    },
    "filtrations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::filtration.filtration",
      "mappedBy": "spirits"
    }
  }
}
